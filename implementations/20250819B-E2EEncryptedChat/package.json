{
  "name": "e2e-encrypted-chat",
  "version": "1.0.0",
  "description": "End-to-End Encrypted Chat Application built with ae-framework",
  "main": "src/integration/orchestrator.ts",
  "scripts": {
    "dev": "tsx watch src/integration/orchestrator.ts",
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration", 
    "test:e2e": "jest --testPathPattern=e2e",
    "test:security": "npm run security:audit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "integrate": "tsx src/integration/orchestrator.ts",
    "storybook": "cd ui && storybook dev -p 6006",
    "security:audit": "npm audit && snyk test",
    "docker:build": "docker build -t e2e-chat:latest .",
    "docker:run": "docker-compose up",
    "migrate": "tsx scripts/migrate.ts",
    "generate:types": "graphql-codegen"
  },
  "keywords": [
    "e2e-encryption",
    "chat",
    "signal-protocol",
    "ae-framework",
    "typescript",
    "react",
    "nextjs"
  ],
  "author": "ae-framework Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@apollo/server": "^4.9.0",
    "argon2": "^0.31.0",
    "graphql": "^16.8.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "libsodium-wrappers": "^0.7.13",
    "next": "^14.0.0",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "tailwindcss": "^3.3.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@storybook/react": "^7.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/itdojp/ae-examples.git"
  },
  "bugs": {
    "url": "https://github.com/itdojp/ae-examples/issues"
  },
  "homepage": "https://github.com/itdojp/ae-examples/tree/main/20250819B-E2EEncryptedChat"
}